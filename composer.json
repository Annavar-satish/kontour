{
  "name": "kontenta/kontour",
  "description": "Kontour is an admin page manager for Laravel",
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": "~5.5"
  },
  "require-dev": {
    "laravel/dusk": "^3.0.2",
    "mockery/mockery": "^1.2",
    "orchestra/testbench": "^3.5",
    "orchestra/testbench-dusk": "^3.5.5|3.6.x-dev",
    "phpunit/phpunit": ">6.5",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Kontenta\\Kontour\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kontenta\\Kontour\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kontenta\\Kontour\\Providers\\KontourServiceProvider"
      ],
      "aliases": {}
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "phpcs": "vendor/bin/phpcs"
  }
}
