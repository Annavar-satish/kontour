{
  "name": "kontenta/kontour-support",
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": "5.5.*|5.6.*|5.7.*"
  },
  "require-dev": {
    "kontenta/kontour": "dev-master",
    "laravel/dusk": ">2.0.13",
    "mockery/mockery": "^1.2",
    "orchestra/testbench": "^3.5",
    "orchestra/testbench-dusk": "^3.5.5|3.6.x-dev",
    "phpunit/phpunit": ">6.5"
  },
  "autoload": {
    "psr-4": {
      "Kontenta\\KontourSupport\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kontenta\\KontourSupport\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kontenta\\KontourSupport\\KontourServiceProvider"
      ],
      "aliases": {}
    }
  },
  "config": {
    "sort-packages": true
  }
}
