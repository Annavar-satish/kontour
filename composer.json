{
  "name": "kontenta/kontour",
  "description": "Kontour is an admin page manager for Laravel",
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": "~5.5",
    "kontenta/kontour-support": "^0.2"
  },
  "autoload": {
    "psr-4": {
      "Kontenta\\Kontour\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kontenta\\Kontour\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3"
  },
  "scripts": {
      "test": "vendor/bin/phpunit"
  }
}
