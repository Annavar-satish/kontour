{
  "name": "kontenta/kontour",
  "description": "Admin area tool utilities for Laravel",
  "license": "MIT",
  "require": {
    "laravel/framework": "5.7.*|5.8.*"
  },
  "require-dev": {
    "laravel/dusk": ">3.0.1",
    "mockery/mockery": "^1.2",
    "orchestra/testbench": "^3.7",
    "orchestra/testbench-dusk": "^3.5.6|3.7.x-dev",
    "squizlabs/php_codesniffer": "^3.3",
    "timacdonald/log-fake": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Kontenta\\Kontour\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kontenta\\Kontour\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kontenta\\Kontour\\Providers\\KontourServiceProvider"
      ],
      "aliases": {}
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "phpcs": "vendor/bin/phpcs"
  }
}
