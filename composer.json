{
  "name": "kontenta/kontour-support",
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "laravel/framework": "~5.5"
  },
  "require-dev": {
    "kontenta/kontour": "dev-master",
    "orchestra/testbench": "^3.5",
    "orchestra/testbench-dusk": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Kontenta\\KontourSupport\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kontenta\\KontourSupport\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kontenta\\KontourSupport\\KontourServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "config": {
    "sort-packages": true
  }
}
